<odoo>

    <record model="ir.ui.view" id="ir_attachment_form_view">
        <field name="name">ir.attachment.form</field>
        <field name="model">ir.attachment</field>
        <field name="inherit_id" ref="base.view_attachment_form" />
        <field name="arch" type="xml">
            <group name="thumbnails" position="after">
                <group name="se_thumbnails" string="Indexed Thumbnails" colspan="4">
                    <field name="se_thumbnail_ids" nolabel="1" colspan="2" />
                </group>
            </group>
        </field>
    </record>



</odoo>
